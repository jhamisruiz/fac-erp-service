{
    "name": "dev-app/app",
    "description": "Plantillas para generar componentes",
    "basename": "dev-app",
    "type": "project",
    "require": {
        "klein/klein": "^2.1",
        "firebase/php-jwt": "^6.2",
        "twig/twig": "3.x-dev",
        "symfony/console": "5.4.x-dev",
        "symfony/contracts": "2.5.x-dev",
        "symfony/process": "5.4.x-dev",
        "symfony/http-foundation": "5.4.x-dev",
        "slim/psr7": "1.6.x-dev",
        "neomerx/cors-psr7": "^2.0",
        "vlucas/phpdotenv": "5.5.x-dev",
        "egulias/email-validator": "^2.1.10",
        "illuminate/validation": "^8.83",
        "rbdwllr/reallysimplejwt": "^4.0",
        "mercadopago/dx-php": "^2.5",
        "phpmailer/phpmailer": "^6.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/api",
            "Trm\\": "app/api/movimientos",
            "DB\\": "app/database",
            "Cmd\\": "app/cmd",
            "Mnt\\": "app/api",
            "Mvt\\": "app/api",
            "Sv\\": "app/endpoints"
        }
